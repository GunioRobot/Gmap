<h1>Editing User</h1>
<table>
<% form_for(@user) do |f| %>
  <%= f.error_messages %>
  <tr><td>Name: </td><td><%= f.text_field :name %></td></tr>
  <tr><td>UserName: </td><td><%= f.text_field :username %></td></tr>
  <tr><td>Email: </td><td><%= f.text_field :email %></td></tr>
  <tr><td>Status:</td><td><%= f.select(:status, {"active" => "active", "expired" => "expired"}) %></td></tr>
  <tr><td><%= f.submit "Update" %></td></tr>
<% end %>
</table>
<%= link_to 'Show', @user %> |
<%= link_to 'Back', users_path %>
