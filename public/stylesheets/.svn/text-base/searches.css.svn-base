
ul.enter_search  {
  margin-top: 20px;
}

ul.enter_search li {
  display: inline;
  font-size: 16px;
  margin: 0px 5px;
}

div.intro {
  padding: 10px;
}

div.intro .sb-inner  {
  background: #f4f4f4;
}

div.intro .sb-border {
  background: #5494b5;
}

ul.search_tabs {
  margin-top: 20px;
  position: relative;
  top: -1px;
}

ul.search_tabs li {
  display: inline;
  padding: 5px;
  margin-right: 2px;
}

ul.search_tabs li .sb-inner {
  background: #2785b5;
}

ul.search_tabs li .sb-border {
  background: #2785b5;
}

ul.search_tabs li a {
  color: #f1f1f1;
}

ul.search_tabs li:hover .sb-inner {
 background: #8ca7b5;
}

div#search_container {
  width: 100%;
  height: 500px;
}

div#search_result {
  width: 400px;
  border: 2px solid #2785b5;
  float: left;
  margin-left: -101%;
  height: 500px;
}

div#map {
  float: left;
  border: 2px solid #2785b5;
  border-left: none;
  width: 100%;
}

div#map_div {
  height: 500px;
  margin-left: 400px;
}

